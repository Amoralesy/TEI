    <desc versionDate="2015-10-26" xml:lang="en" xml:id="msfrag.desc">contains information about a fragment now kept as a single unit or in a larger manuscript.</desc>
    <desc versionDate="2007-12-20" xml:lang="ko"></desc>
    <desc versionDate="2007-05-02" xml:lang="zh-TW"></desc>
    <desc versionDate="2008-04-05" xml:lang="ja"></desc>
